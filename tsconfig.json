{
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "declaration": false,
    "noResolve": false,
    "jsx": "react",
    "reactNamespace": "RX",
    "module": "commonjs",
    "target": "es5",
    "lib": [
      "es6",
      "dom"
    ],
    "experimentalDecorators": true,
    "sourceMap": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "outDir": "./dist/",
    "types": [
      "node",
      "sax",
      "pako",
      "lodash"
    ]
  },
  "include": [
    "./src/**/*"
  ]
}